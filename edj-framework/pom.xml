            </dependency>
            <dependency>
                <groupId>com.edj</groupId>
                <artifactId>edj-cache</artifactId>
                <version>${edj.version}</version>
            </dependency>
            <dependency>
                <groupId>com.edj</groupId>
                <artifactId>edj-api</artifactId>
